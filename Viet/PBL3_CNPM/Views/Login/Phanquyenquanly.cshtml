            --padding-37xl: 56px;
            --padding-8xs: 5px;
            --padding-10xs-1: 2.1px;
            --padding-lgi-7: 19.7px;
            /* Border radiuses */
            --br-8xs: 5px;
            --br-xl: 20px;
            --br-mini: 15px;
            --br-5xs: 8px;
